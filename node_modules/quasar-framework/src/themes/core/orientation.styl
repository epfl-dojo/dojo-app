.rotate-45
  transform rotate(45deg)
.rotate-90
  transform rotate(90deg)
.rotate-135
  transform rotate(135deg)
.rotate-180
  transform rotate(180deg)
.rotate-205
  transform rotate(205deg)
.rotate-270
  transform rotate(270deg)
.flip-horizontal
  transform scale(-1, 1)
.flip-vertical
  transform scale(1, -1)
.transition-generic
  transition all .3s
